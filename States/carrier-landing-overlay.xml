<PropertyList>
    <name type="string" n="0">carrier-approach</name>
    <description type="string">Final approach to a carrier landing.  Everything out - gear, flaps, speed-brakes, tailhook</description>
    <overlay>
	<controls>
	    <engines>
		<engine n="0">
		    <throttle>0.75</throttle>
		    <cutoff type="bool">false</cutoff>
		    <starter type="bool">false</starter>
		</engine>
		<engine n="1">
		    <throttle>0.75</throttle>
                    <cutoff type="bool">false</cutoff>
                    <starter type="bool">false</starter>
		</engine>
	    </engines>
	    <flight>
		<elevator-trim>-0.521</elevator-trim>
		<flaps>0.5</flaps>
		<spoilers>1.0</spoilers>
		<wing-fold>
		    <handle-cmd>0</handle-cmd>
		    <lockpin-cmd>0</lockpin-cmd>
		</wing-fold>
	    </flight>
	    <gear>
		<tailhook>true</tailhook>
		<gear-down>true</gear-down>
		<parking-brake>0.0</parking-brake>
	    </gear>
	    <lighting>
  		<beacon>1</beacon>
  		<taxi-light>0</taxi-light>
	    </lighting>
	</controls>

	<engines>
            <engine n="0">
                <running type="bool">true</running>
            </engine>
            <engine n="1">
                <running type="bool">true</running>
            </engine>
        </engines>

	<canopy>
	    <position-norm>0.0</position-norm>
	</canopy>

	<fdm>
	    <jsbsim>
		<fcs>
		    <wing-fold>
			<lockpin>1</lockpin>
		    </wing-fold>
		</fcs>
	    </jsbsim>
	</fdm>
	
	<orientation>
	    <pitch-deg>11</pitch-deg>
	</orientation>
    </overlay>
</PropertyList>
