<?xml version="1.0"?>

	<PropertyList>

	<path>light-left-panel.ac</path>

	<!-- collision-switch -->
	<animation>
		<object-name>collision-switch</object-name>
		<type>rotate</type>
		<property>controls/lighting/beacon</property>
		<interpolation>
			<entry><ind>0</ind><dep>-10</dep></entry>
			<entry><ind>1</ind><dep>25</dep></entry>
		</interpolation>
		<center><x-m>-0.0567</x-m><z-m>0.0045</z-m></center>
		<axis><y>-1</y></axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>collision-switch</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/lighting/beacon</property>
			</binding>
		</action>
	</animation>

	<!-- taxi/probe-switch -->
	<animation>
		<object-name>taxi-probe-switch</object-name>
		<type>rotate</type>
		<property>controls/lighting/taxi-light</property>
		<interpolation>
			<entry><ind>0</ind><dep>-10</dep></entry>
			<entry><ind>1</ind><dep>25</dep></entry>
		</interpolation>
		<center><x-m>-0.0143</x-m><z-m>0.0045</z-m></center>
		<axis><y>-1</y></axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>taxi-probe-switch</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/lighting/taxi-light</property>
			</binding>
		</action>
	</animation>

	<!-- pylon-switch -->
	<animation>
		<object-name>pylon-switch</object-name>
		<type>rotate</type>
		<property>sim/model/A-6E/controls/lighting/pylons-lts</property>
		<interpolation>
			<entry><ind>0</ind><dep>-30</dep></entry>
			<entry><ind>1</ind><dep>0</dep></entry>
			<entry><ind>2</ind><dep>30</dep></entry>
		</interpolation>
		<center><x-m>0.0586</x-m><z-m>0.0045</z-m></center>
		<axis><y>-1</y></axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>pylon-switch</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
						<command>nasal</command>
						<script>A6Econtrols.three_pos_sw("sim/model/A-6E/controls/lighting/pylons-lts", 1)</script>
			</binding>
		</action>
		<action>
			<button>1</button>
			<repeatable>false</repeatable>
			<binding>
						<command>nasal</command>
						<script>A6Econtrols.three_pos_sw("sim/model/A-6E/controls/lighting/pylons-lts", -1)</script>
			</binding>
		</action>
	</animation>

	<!-- wheels -->
	<animation>
		<type>rotate</type>
		<object-name>instrument-wheel</object-name>
		<property>controls/lighting/instruments-norm</property>
		<factor>336</factor>
		<center><y-m>0.0002</y-m><z-m>-0.0101</z-m></center>
		<axis><x>1</x></axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>instrument-wheel</object-name>
		<action>
			<button>0</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/lighting/instruments-norm</property>
				<step>0.05</step>
			</binding>
		</action>
		<action>
			<button>1</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/lighting/instruments-norm</property>
				<step>-0.05</step>
			</binding>
		</action>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>console-wheel</object-name>
		<property>controls/lighting/panel-norm</property>
		<factor>336</factor>
		<center><y-m>0.0002</y-m><z-m>-0.0101</z-m></center>
		<axis><x>1</x></axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>console-wheel</object-name>
		<action>
			<button>0</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/lighting/panel-norm</property>
				<step>0.05</step>
			</binding>
		</action>
		<action>
			<button>1</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/lighting/panel-norm</property>
				<step>-0.05</step>
			</binding>
		</action>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>approach-index-wheel</object-name>
		<property>sim/model/A-6E/controls/lighting/index-norm</property>
		<factor>336</factor>
		<center><y-m>0.0002</y-m><z-m>-0.0101</z-m></center>
		<axis><x>1</x></axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>approach-index-wheel</object-name>
		<action>
			<button>0</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>sim/model/A-6E/controls/lighting/index-norm</property>
				<step>0.05</step>
				<min>0</min>
				<max>1</max>
			</binding>
		</action>
		<action>
			<button>1</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>sim/model/A-6E/controls/lighting/index-norm</property>
				<step>-0.05</step>
				<min>0</min>
				<max>1</max>
			</binding>
		</action>
	</animation>

	<!-- tail-switch -->
	<animation>
		<object-name>tail-switch</object-name>
		<type>rotate</type>
		<property>sim/model/A-6E/controls/lighting/tail-lt</property>
