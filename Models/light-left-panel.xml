		</action>
		<action>
			<button>1</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>A6Econtrols.three_pos_sw_flood("sim/model/A-6E/controls/lighting/flood-lts", -1)</script>
			</binding>
		</action>
	</animation>

	<!-- lighting -->
	<animation> 
		<object-name>mount</object-name>
		<type>material</type>
		<emission> 
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.45</red>
			<green>0.34</green>
			<blue>0.18</blue>
		</emission>
	</animation>
	<animation>
		<object-name>switches-and-knobs</object-name>
		<type>material</type>
		<emission> 
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.50</red>
			<green>0.25</green>
			<blue>0.18</blue>
		</emission>
	</animation>


	<animation>
		<object-name>light-left-panel</object-name>
		<type>noshadow</type>
	</animation>


	<!-- LoD -->
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>35</max-m>
	</animation>

</PropertyList>
