	    <entry><ind>0</ind><dep>0</dep></entry>
	    <entry><ind>10</ind><dep>1.5</dep></entry>
	    <entry><ind>20</ind><dep>4</dep></entry>
	    <entry><ind>40</ind><dep>10</dep></entry>
	    <entry><ind>80</ind><dep>21</dep></entry>
	    <entry><ind>100</ind><dep>46</dep></entry>
	    <entry><ind>120</ind><dep>73</dep></entry>
