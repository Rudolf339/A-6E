<?xml version="1.0"?>

<PropertyList>

  <name>ASI</name>
  <path>asi.ac</path>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <interpolation>
      <entry><ind>0</ind><dep>0</dep></entry>
      <entry><ind>10</ind><dep>1.5</dep></entry>
      <entry><ind>20</ind><dep>4</dep></entry>
      <entry><ind>40</ind><dep>10</dep></entry>
      <entry><ind>80</ind><dep>21</dep></entry>
      <entry><ind>100</ind><dep>44</dep></entry>
      <entry><ind>120</ind><dep>73</dep></entry>
      <entry><ind>150</ind><dep>112</dep></entry>
      <entry><ind>200</ind><dep>148</dep></entry>
      <entry><ind>250</ind><dep>182</dep></entry>
      <entry><ind>300</ind><dep>212</dep></entry>
      <entry><ind>350</ind><dep>240</dep></entry>
      <entry><ind>400</ind><dep>266</dep></entry>
      <entry><ind>450</ind><dep>290</dep></entry>
      <entry><ind>500</ind><dep>310</dep></entry>
      <entry><ind>600</ind><dep>335</dep></entry>
    </interpolation>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>mach-scale</object-name>
    <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <interpolation>
      <entry><ind>0</ind><dep>0</dep></entry>
      <entry><ind>10</ind><dep>1.5</dep></entry>
      <entry><ind>20</ind><dep>4</dep></entry>
      <entry><ind>40</ind><dep>10</dep></entry>
      <entry><ind>80</ind><dep>21</dep></entry>
      <entry><ind>100</ind><dep>44</dep></entry>
      <entry><ind>120</ind><dep>73</dep></entry>
      <entry><ind>150</ind><dep>112</dep></entry>
      <entry><ind>200</ind><dep>148</dep></entry>
      <entry><ind>250</ind><dep>182</dep></entry>
      <entry><ind>300</ind><dep>212</dep></entry>
      <entry><ind>350</ind><dep>240</dep></entry>
      <entry><ind>400</ind><dep>266</dep></entry>
      <entry><ind>450</ind><dep>290</dep></entry>
      <entry><ind>500</ind><dep>310</dep></entry>
      <entry><ind>600</ind><dep>335</dep></entry>
    </interpolation>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>


  <animation>
    <type>rotate</type>
    <object-name>mach-scale</object-name>
    <property>velocities/mach</property>
    <interpolation>
      <entry><ind>0</ind><dep>40</dep></entry>
      <entry><ind>0.1</ind><dep>60</dep></entry>
      <entry><ind>0.2</ind><dep>80</dep></entry>
      <entry><ind>0.3</ind><dep>107.4</dep></entry>
      <entry><ind>0.4</ind><dep>129.2</dep></entry>
      <entry><ind>0.5</ind><dep>150.6</dep></entry>
      <entry><ind>0.6</ind><dep>166.2</dep></entry>
      <entry><ind>0.7</ind><dep>182.6</dep></entry>
      <entry><ind>0.8</ind><dep>196</dep></entry>
      <entry><ind>0.9</ind><dep>209.8</dep></entry>
      <entry><ind>1.0</ind><dep>223.5</dep></entry>
      <entry><ind>1.1</ind><dep>246</dep></entry>
      <entry><ind>1.2</ind><dep>266</dep></entry>
      <entry><ind>1.3</ind><dep>255</dep></entry>
      <entry><ind>1.4</ind><dep>264</dep></entry>
      <entry><ind>1.5</ind><dep>274</dep></entry>
      <entry><ind>1.6</ind><dep>281</dep></entry>
    </interpolation>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>safe-speed-bug</object-name>
    <property>sim/model/A-6E/instrumentation/airspeed-indicator/safe-speed-limit-bug</property>
    <interpolation>
      <entry><ind>0</ind><dep>0</dep></entry>
      <entry><ind>10</ind><dep>1.5</dep></entry>
      <entry><ind>20</ind><dep>4</dep></entry>
      <entry><ind>40</ind><dep>10</dep></entry>
      <entry><ind>80</ind><dep>21</dep></entry>
      <entry><ind>100</ind><dep>44</dep></entry>
      <entry><ind>120</ind><dep>73</dep></entry>
      <entry><ind>150</ind><dep>112</dep></entry>
      <entry><ind>200</ind><dep>148</dep></entry>
      <entry><ind>250</ind><dep>182</dep></entry>
      <entry><ind>300</ind><dep>212</dep></entry>
      <entry><ind>350</ind><dep>240</dep></entry>
      <entry><ind>400</ind><dep>266</dep></entry>
      <entry><ind>450</ind><dep>290</dep></entry>
      <entry><ind>500</ind><dep>310</dep></entry>
      <entry><ind>600</ind><dep>335</dep></entry>
    </interpolation>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

<!-- Air Speed Indicator's Bug Setting -->
  <animation>
    <name>asi-bug-left-fine</name>
    <type>pick</type>
    <visible>true</visible>
    <object-name>asi-bug-left</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.2</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>sim/model/A-6E/instrumentation/airspeed-indicator/safe-speed-limit-bug</property>
        <step>-10</step>
        <min>180</min>
      </binding>
    </action>
  </animation>
  <animation>
    <name>asi-bug-left-coarse</name>
    <type>pick</type>
    <visible>true</visible>
    <object-name>asi-bug-left</object-name>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <interval-sec>0.2</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>sim/model/A-6E/instrumentation/airspeed-indicator/safe-speed-limit-bug</property>
        <step>-50</step>
        <min>180</min>
      </binding>
    </action>
  </animation>

  <animation>
    <name>asi-bug-right-fine</name>
    <type>pick</type>
    <visible>true</visible>
    <object-name>asi-bug-right</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.2</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>sim/model/A-6E/instrumentation/airspeed-indicator/safe-speed-limit-bug</property>
        <step>10</step>
        <max>560</max>
      </binding>
    </action>
  </animation>
  <animation>
    <name>asi-bug-right-coarse</name>
    <type>pick</type>
    <visible>true</visible>
    <object-name>asi-bug-right</object-name>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <interval-sec>0.2</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>sim/model/A-6E/instrumentation/airspeed-indicator/safe-speed-limit-bug</property>
        <step>50</step>
        <max>560</max>
      </binding>
    </action>
  </animation>

  <!-- Lightning -->
  <animation>
    <object-name>asi</object-name>
    <type>material</type>
    <condition><not><property>sim/rendering/rembrandt/enabled</property></not></condition>
    <emission>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
      <red>0.55</red>
      <green>0.24</green>
      <blue>0.10</blue>
    </emission>
  </animation>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>15</max-m>
  </animation>

</PropertyList>
