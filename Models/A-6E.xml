    <!-- ######### Right Gear ########### -->
    <!-- Extend/Retract -->
    <animation>
	<type>rotate</type>
	<object-name>UC-R-StrutTop</object-name>
	<object-name>UC-R-StrutShaft</object-name>
	<object-name>UC-R-StrutShaft-cont</object-name>
	<object-name>UC-R-Hub</object-name>
	<object-name>UC-R-Tyre</object-name>
	<object-name>UC-R-TopDamper</object-name>
	<object-name>UC-R-BottomDamper</object-name>
	<object-name>UC-R-BottomArm</object-name>
	<object-name>UC-R-TopArm</object-name>
	<property>gear/gear[2]/position-norm</property>
	<interpolation>
	    <entry><ind>0</ind><dep>93.75</dep></entry>
	    <entry><ind>0.3</ind><dep>93.75</dep></entry>
	    <entry><ind>1</ind><dep>0</dep></entry>
	</interpolation>
	<axis>
	    <object-name>axis44</object-name>
	</axis>
    </animation>

    <animation>
	<type>rotate</type>
	<object-name>UC-R-OpeningJackTop</object-name>
	<object-name>UC-R-OpeningJackBottom</object-name>
	<property>gear/gear[2]/position-norm</property>
	<interpolation>
	    <entry><ind>0</ind><dep>75</dep></entry>
	    <entry><ind>0.30</ind><dep>75</dep></entry>
	    <entry><ind>0.485</ind><dep>58.5</dep></entry>
	    <entry><ind>0.67</ind><dep>40</dep></entry>
	    <entry><ind>1</ind><dep>0</dep></entry>
	</interpolation>
	<axis>
	    <object-name>axis45</object-name>
	</axis>
    </animation>

    <animation>
	<type>rotate</type>
	<object-name>UC-R-StrutShaft-cont</object-name>
	<object-name>UC-R-Hub</object-name>
	<object-name>UC-R-Tyre</object-name>
	<object-name>UC-R-TopDamper</object-name>
	<object-name>UC-R-BottomDamper</object-name>
	<object-name>UC-R-BottomArm</object-name>
	<object-name>UC-R-TopArm</object-name>
	<property>gear/gear[2]/position-norm</property>
	<interpolation>
	    <entry><ind>0</ind><dep>86</dep></entry>
