<?xml version="1.0"?>

<!-- A-6E model config file -->
<!-- Alexis Bory 2012/07/12 -->

<PropertyList>

    <path>A-6E.ac</path>

    <model>
	<path>Aircraft/A-6E/Models/A-6E-cockpit.xml</path>
	<offsets>
	    <z-m>-0.3</z-m>
	    <pitch-deg>-3</pitch-deg>
	</offsets>
    </model>

    <!-- ####### Select the external loads as appropriate ###### -->
    <!-- Select extermal tanks -->
    <animation>
	<type>select</type>
	<object-name>tank-1</object-name>
	<condition>
	    <and>
		<equals>
		    <property>sim/multiplay/generic/string</property>
		    <value>aero300</value>
		</equals>
		<equals>
		    <property>sim/multiplay/generic/int[3]</property>
		    <value>1</value>
		</equals>
	    </and>
	</condition>
    </animation>
    <animation>
	<type>select</type>
	<object-name>tank-2</object-name>
	<condition>
	    <and>
		<equals>
		    <property>sim/multiplay/generic/string[1]</property>
		    <value>aero300</value>
		</equals>
		<equals>
		    <property>sim/multiplay/generic/int[4]</property>
		    <value>1</value>
		</equals>
	    </and>
	</condition>
    </animation>
    <animation>
	<type>select</type>
	<object-name>tank-3</object-name>
	<condition>
	    <and>
		<equals>
		    <property>sim/multiplay/generic/string[2]</property>
		    <value>aero300</value>
		</equals>
		<equals>
		    <property>sim/multiplay/generic/int[5]</property>
		    <value>1</value>
		</equals>
	    </and>
	</condition>
    </animation>
    <animation>
	<type>select</type>
	<object-name>tank-4</object-name>
	<condition>
	    <and>
		<equals>
		    <property>sim/multiplay/generic/string[3]</property>
		    <value>aero300</value>
		</equals>
		<equals>
		    <property>sim/multiplay/generic/int[6]</property>
		    <value>1</value>
		</equals>
	    </and>
	</condition>
    </animation>
    <animation>
	<type>select</type>
	<object-name>tank-5</object-name>
	<condition>
	    <and>
		<equals>
		    <property>sim/multiplay/generic/string[4]</property>
		    <value>aero300</value>
		</equals>
		<equals>
		    <property>sim/multiplay/generic/int[7]</property>
		    <value>1</value>
		</equals>
	    </and>
	</condition>
    </animation>

    <!-- ###### De-select when in Cockpit View ###### -->
    <animation>
	<type>select</type>
	<object-name>L-Pilot</object-name>
	<condition>
	    <not>
		<equals>
		    <property>sim/current-view/name</property>
		    <value>Cockpit View</value>
		</equals>
	    </not>
	</condition>
    </animation>
    <!-- ###### De-select when in Navigator View ###### -->
    <animation>
	<type>select</type>
	<object-name>R-Pilot</object-name>
	<condition>
	    <not>
		<equals>
		    <property>sim/current-view/name</property>
		    <value>Navigator View</value>
		</equals>
	    </not>
	</condition>
    </animation>
    <!-- ###### De-select when in Cockpit View or in Navigator View ###### -->
    <animation>
	<type>select</type>
	<object-name>canopy-main-glass</object-name>
	<object-name>windshield-side-glass</object-name>
	<object-name>windshield-front-glass</object-name>
	<object-name>windshield-top-glass</object-name>
	<condition>
	    <not-equals>
		<property>sim/current-view/name</property>
		<value>Cockpit View</value>
	    </not-equals>
	    <not-equals>
		<property>sim/current-view/name</property>
		<value>Navigator View</value>
	    </not-equals>
	</condition>
    </animation>

    <animation>
	<type>shader</type>
	<shader>chrome</shader>
	<texture>Aircraft/A-6E/Models/glass_shader.png</texture>
	<object-name>glass</object-name>
    </animation>



