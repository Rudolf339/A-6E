    <!-- ########################################################### -->

    <!-- ########## Left Pilot's right arm ########### -->
    <animation>
	<type>rotate</type>
	<object-name>LR-armA</object-name>
	<property>controls/flight/elevator</property>
	<factor>22</factor>
	<axis>
	    <object-name>axis0</object-name>
	</axis>
    </animation>
