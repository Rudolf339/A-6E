<?xml version="1.0"?>
<PropertyList>

 <path>accelerometer2.ac</path>

  <animation>
    <type>translate</type>
    <object-name>needle</object-name>
    <property>accelerations/pilot-g</property>
    <interpolation>
      <entry><ind>-6</ind><dep>-0.0270</dep></entry>
      <entry><ind>0</ind><dep>0</dep></entry>
      <entry><ind>11</ind><dep>0.0522</dep></entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>


<!-- lighting -->

  <animation>
    <object-name>accel2</object-name>
    <type>material</type>
    <condition><not><property>sim/rendering/rembrandt/enabled</property></not></condition>
    <emission>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
        <red>0.55</red>
        <green>0.24</green>
        <blue>0.10</blue>
    </emission>
  </animation>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>15</max-m>
  </animation>

</PropertyList>
