<?xml version="1.0"?>


<PropertyList>

	<path>phd.ac</path>

	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>plots</object-name>
		<object-name>radar-frame</object-name>
		<object-name>bkg</object-name>
	</effect>

	<animation>
		<type>rotate</type>
		<object-name>compass-card</object-name>
		<property>orientation/heading-magnetic-deg</property>
		<center>
			<x-m>0.0</x-m>
			<y-m>0.0</y-m>
			<z-m>0.0</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>select</type>
		<object-name>plots</object-name>
		<condition>
			<and>
				<equals>
					<property>sim/model/A-6E/controls/PHD/off-stdby-on</property>
					<value>2</value>
				</equals>
				<not>
					<property>sim/model/A-6E/instrumentation/PHD/plots-out-of-range</property>
				</not>
			</and>
		</condition>
	</animation>
 
	<animation>
		<type>select</type>
		<object-name>bkg-off</object-name>
		<condition>
			<equals>
				<property>sim/model/A-6E/controls/PHD/off-stdby-on</property>
				<value>0</value>
			</equals>
		</condition>
	</animation>
 
	<animation>
		<type>select</type>
		<object-name>radar-frame</object-name>
		<condition>
			<greater-than>
				<property>sim/model/A-6E/controls/PHD/off-stdby-on</property>
				<value>0</value>
			</greater-than>
		</condition>
	</animation>
 
	<animation>
		<type>translate</type>
		<object-name>plots</object-name>
		<property>sim/model/A-6E/instrumentation/PHD/v-deviation</property>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>-1</z>
		</axis>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>plots</object-name>
		<property>sim/model/A-6E/instrumentation/PHD/h-deviation</property>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

