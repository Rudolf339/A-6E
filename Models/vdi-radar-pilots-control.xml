	<!-- PHD on-stby-off-switch -->
	<animation>
		<object-name>horiz-on-stby-off-switch</object-name>
		<type>rotate</type>
		<property>sim/model/A-6E/controls/PHD/off-stdby-on</property>
		<interpolation>
			<entry><ind>0</ind><dep>-30</dep></entry>
			<entry><ind>1</ind><dep>0</dep></entry>
			<entry><ind>2</ind><dep>30</dep></entry>
		</interpolation>
		<center>
			<x-m>0.0000</x-m>
			<y-m>0.0409</y-m>
			<z-m>-0.0090</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

	<!-- PHD TC range switch -->
	<animation>
		<object-name>scale-switch</object-name>
		<type>rotate</type>
		<property>sim/model/A-6E/controls/PHD/TCrange-switch</property>
		<interpolation>
			<entry><ind>0</ind><dep>-30</dep></entry>
			<entry><ind>1</ind><dep>0</dep></entry>
			<entry><ind>2</ind><dep>30</dep></entry>
		</interpolation>
		<center>
			<x-m>0.0000</x-m>
			<y-m>0.0000</y-m>
			<z-m>-0.1227</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

